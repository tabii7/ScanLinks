
# OpenAI Rate Limit Protection
# Increase delay between OpenAI requests (in milliseconds)
# 60000 = 60 seconds (1 request/min) - very conservative for free tier
# 120000 = 120 seconds (1 request/2min) - extremely conservative
OPENAI_REQUEST_INTERVAL=60000

# Enable scan queue to automatically space out scans
# When enabled, scans are queued and processed with delays
ENABLE_SCAN_QUEUE=false
